datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Animal {
  id    String @id @default(uuid())
  title String
  text  String

  createdAt DateTime @unique @default(now())
  updatedAt DateTime @unique @default(now())
}

