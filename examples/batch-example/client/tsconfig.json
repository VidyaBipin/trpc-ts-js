{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "strict": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "types": [
    ],
    "baseUrl": ".",
    "lib": [
      "dom",
      "ES2021"
    ],
  },
  "include": ["./"],
  "exclude": ["node_modules"]
}
